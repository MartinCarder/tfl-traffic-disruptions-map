{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": [0],
    "react/jsx-boolean-value": 1,
    "global-require": 0,
    "max-len": [2, 100, 2, {ignoreComments: true}],
    "no-param-reassign": ["error", { "props": false }],
    "jsx-a11y/no-static-element-interactions": 'off',
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  }
}
